model User {
  id        String    @id @default(auto()) @map("_id") @db.ObjectId
  email     String    @unique
  name      String?
  createdAt DateTime  @default(now())
  createdBy String    @db.ObjectId
  updatedAt DateTime?
  updatedBy String?   @db.ObjectId
}

model UserChangeLog {
  id         String    @id @default(auto()) @map("_id") @db.ObjectId
  userId     String    @db.ObjectId
  email      String?
  name       String?
  createdAt  DateTime?
  createdBy  String?   @db.ObjectId
  updatedAt  DateTime?
  updatedBy  String?   @db.ObjectId
  archivedAt DateTime?
  archivedBy String?   @db.ObjectId
}
